<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
    http://www.springframework.org/schema/beans   
    http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-3.1.xsd
">
	
<bean id="TipoMovimientoIngresoAlta" class="models.Tipo_Movimiento">
	<property name="Nombre" value="Alta"></property>
</bean>

<bean id="TipoMovimientoIngresoIngreso" class="models.Tipo_Movimiento">
	<property name="Nombre" value="Ingreso"></property>
</bean>

<bean id="TipoMovimientoIngresoEgreso" class="models.Tipo_Movimiento">
	<property name="Nombre" value="Egreso"></property>
</bean>

</beans>