<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
    http://www.springframework.org/schema/beans   
    http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-3.1.xsd
">

<bean id="PaisArgentina" class="models.Pais">
	<property name="Nombre" value="Argentina"></property>
</bean>

<bean id="PaisBolivia" class="models.Pais">
	<property name="Nombre" value="Bolivia"></property>
</bean>

<bean id="PaisChile" class="models.Pais">
	<property name="Nombre" value="Chile"></property>
</bean>

<bean id="PaisBrasil" class="models.Pais">
	<property name="Nombre" value="Brasil"></property>
</bean>

<bean id="PaisMexico" class="models.Pais">
	<property name="Nombre" value="Mexico"></property>
</bean>

<bean id="PaisEstadosUnidos" class="models.Pais">
	<property name="Nombre" value="Estados Unidos"></property>
</bean>

<bean id="PaisInglaterra" class="models.Pais">
	<property name="Nombre" value="Inglaterra"></property>
</bean>

<bean id="PaisEspaña" class="models.Pais">
	<property name="Nombre" value="España"></property>
</bean>

<bean id="PaisCostaRica" class="models.Pais">
	<property name="Nombre" value="Costa rica"></property>
</bean>

<bean id="PaisPeru" class="models.Pais">
	<property name="Nombre" value="Peru"></property>
</bean>

</beans>